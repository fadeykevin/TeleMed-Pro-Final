      "package": "com.kevin2424.TeleMedProFinal",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0088CC"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-mail-composer"
    ],
    "sdkVersion": "54.0.0",
    "extra": {
      "eas": {
        "projectId": "79cda460-fcdd-4de9-bc77-d62cd1187d29"
      }
    }
  }
